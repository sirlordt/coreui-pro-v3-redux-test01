{
  "extends": [
     "airbnb",
     "plugin:jsx-a11y/recommended",
     "eslint:recommended"
  ],
  "plugins":[ "jsx-a11y" ],
  "parser": "babel-eslint",
  "env":{
    "browser": true
   },
   "rules": {
     "no-undef": "error",
     "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
     "spaced-comment": "off",
     "padded-blocks": ["error", "always"],
     "space-in-parens": [ "error", "always" ],
     "max-len": ["error", { "code": 255 }],
     "arrow-body-style": "off",
     "prefer-destructuring": "off",
     "no-underscore-dangle": "off",
     "computed-property-spacing": ["error", "always"],
     "quotes": ["error", "double"],
     "quote-props": "off",
     "brace-style": ["error", "stroustrup"],
     "operator-linebreak": ["error", "after"],
     "array-bracket-spacing": ["error", "always"],
     "no-else-return": "off",
     "no-plusplus": "off",
     "no-await-in-loop": "off",
     "indent": [ "error", 2, { "ignoredNodes": [ "ExportDeclaration" ], "FunctionDeclaration": {"parameters": "first"} , "CallExpression": {"arguments": "first"}, "SwitchCase": 1 } ],
     "no-console": "off",
     "react/destructuring-assignment": "off",
     "react/prefer-stateless-function": "off",
     "react/forbid-prop-types": "off",
     "react/jsx-curly-spacing": [ 2, { "when": "always", "allowMultiline": false } ],
     "react/jsx-no-bind": [ 1, {
       "ignoreDOMComponents": true,
       "ignoreRefs": true,
       "allowArrowFunctions": true,
       "allowFunctions": true,
       "allowBind": true
     }],
     "react/prop-types": [0, { "ignore": [], "customValidators": [] }],
     "react/jsx-props-no-spreading": "off",
     "import/no-extraneous-dependencies": "off",
     "react/sort-comp": "off",
     "no-unused-expressions": "off",
     "react/no-unused-state": "off",
     "react/no-unused-prop-types": "off",
     "react/require-default-props": "off",
     "class-methods-use-this": "off",
     "no-param-reassign": "off",
     "no-alert": "off",
     "object-curly-newline": ["error", { "multiline": true, "minProperties": 1 }],
     "comma-dangle": ["error", "never"],
     "no-useless-constructor": "off",
     "react/jsx-fragments": [ 0 ],
     "react/jsx-closing-bracket-location": [ 0 ],
     "lines-between-class-members": "off"
   }
}
